from building import *
import rtconfig

# get current directory
cwd     = GetCurrentDir()
# The set of source files associated with this SConscript file.
src     = Glob('src/*.cpp')
path    = [cwd + '/inc']

group = DefineGroup('CFF/HubMotor', src, depend = ['PKG_USING_CFF_HUB_MOTOR'], CPPPATH = path)

Return('group')
