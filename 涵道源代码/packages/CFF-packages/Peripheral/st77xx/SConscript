from building import *
import rtconfig

# get current directory
cwd     = GetCurrentDir()
# The set of source files associated with this SConscript file.
src     = Glob('src/*.c')
path    = [cwd + '/inc']

group = DefineGroup('CFF/st77xx', src, depend = ['PKG_USING_CFF_LCD'], CPPPATH = path)

Return('group')
